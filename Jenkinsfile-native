
//echo "Hello World"
stage("Run & Test Web-Server") {
    parallel 'run': {
        stage("Run WebServer") {
            node('master') {
                sh "sbt run"
            }
        }
            
    }, 'test': {
        stage("Test Web Server") {
            node('master') {
                sh "curl localhost:9000"
            }
        }            
    }
}


